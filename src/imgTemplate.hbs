{{#each this}}
<li class="photo-card">
    <div>
        <a href="#" class="photo_link"><img class="photo-card__img" src="{{this.webformatURL}}" alt="{{this.tags}}"
                data-source="{{this.largeImageURL}}" width="100%" /></a>
        {{!-- {{this.largeImageURL}} --}}
        <div class="stats" data-source="{{this.largeImageURL}}">
            <p class="stats-item">
                <i class="material-icons md-18">thumb_up</i>
                {{this.likes}}
            </p>
            <p class="stats-item">
                <i class="material-icons md-18">visibility</i>
                {{this.views}}
            </p>
            <p class="stats-item">
                <i class="material-icons md-18">comment</i>
                {{this.comments}}
            </p>
            <p class="stats-item">
                <i class="material-icons md-18">cloud_download</i>
                {{this.downloads}}
            </p>
        </div>
    </div>
</li>
{{/each}}